{
  "name": "dreamstate-project",
  "version": "1.0.0",
  "description": "Store management library based on context and observers",
  "private": true,
  "author": "Syrotenko Igor",
  "license": "MIT",
  "scripts": {
    "purge": "npx rimraf ./node_modules */node_modules */**/node_modules",
    "clean": "rimraf ./target",
    "typecheck": "tsc --noEmit",
    "build:cjs": "rollup -c cli/build/rollup.cjs.config.ts --configPlugin typescript --bundleConfigAsCjs",
    "build:esm": "rollup -c cli/build/rollup.esm.config.ts --configPlugin typescript --bundleConfigAsCjs",
    "build:dts": "rollup -c cli/build/rollup.dts.config.ts --configPlugin typescript --bundleConfigAsCjs",
    "build:ptb": "rollup -c cli/build/rollup.esm.portable.config.ts --configPlugin typescript --bundleConfigAsCjs",
    "build:pkg": "ts-node cli/deploy/prepare.ts",
    "build": "pnpm build:dts && pnpm build:esm && pnpm build:cjs && pnpm build:pkg",
    "format": "prettier --write \"**/*.(js|ts|tsx)\" && eslint . --fix",
    "lint": "eslint .",
    "test": "jest --config cli/test/jest.config.js --detectOpenHandles --maxWorkers=100%",
    "pre-deploy": "pnpm clean && pnpm lint && pnpm typecheck && pnpm test && pnpm build",
    "deploy": "pnpm pre-deploy && cd ./target/pkg && npm publish",
    "deploy:experimental": "pnpm pre-deploy && cd ./target/pkg && npm publish --tag experimental"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.1",
    "@babel/node": "^7.27.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/enzyme": "^3.10.19",
    "@types/jest": "27.5.2",
    "@types/ncp": "^2.0.8",
    "@types/node": "^22.15.18",
    "@types/react": "16.14.62",
    "@types/react-dom": "16.9.25",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "babel-jest": "27.5.1",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^9.27.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.17",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^15.15.0",
    "jest": "27.5.1",
    "pnpm": "^10.11.0",
    "prettier": "^3.5.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.40.2",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-import-alias": "^1.0.10",
    "rollup-plugin-visualizer": "^5.14.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  }
}
