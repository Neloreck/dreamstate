{
  "name": "dreamstate",
  "version": "2.1.0",
  "description": "Store manager for react context based on OOP.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "private": false,
  "scripts": {
    "build": "npm run clean && mkdir ./lib && npm run build:tsc && npm run build:switcher && npm run build:dev && npm run build:prod && npm run include:dts && rm -rf ./target",
    "clean": "rm -rf ./lib && rm -rf ./target",
    "include:dts": "cp ./target/dreamstate.d.ts ./lib/index.d.ts",
    "build:tsc": "tsc -p ./src/tsconfig.json",
    "build:switcher": "npx babel target/index.js --out-file lib/index.js --minified --compact true --no-comments --env-name production",
    "build:prod": "npx babel target/dreamstate.js --out-file lib/dreamstate.production.min.js --minified --compact true --no-comments --env-name production",
    "build:dev": "npx babel target/dreamstate.js --out-file lib/dreamstate.development.js --env-name development",
    "=============================": "================================================================================",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neloreck/dreamstate.git"
  },
  "keywords": [
    "action",
    "class-based",
    "declarative",
    "decorator",
    "metadata",
    "react",
    "reducer",
    "redux",
    "state",
    "typescript",
    "utility"
  ],
  "author": "Syrotenko Igor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neloreck/dreamstate/issues"
  },
  "homepage": "https://github.com/neloreck/dreamstate#readme",
  "peerDependencies": {
    "react": "^16.8.3"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.3.0",
    "shallow-equal": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.2.2",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.16",
    "@types/hoist-non-react-statics": "^3.3.1",
    "babel-preset-minify": "^0.5.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "typescript": "^3.3.0"
  }
}
