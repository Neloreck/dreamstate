{
  "name": "dreamstate",
  "version": "2.4.12",
  "description": "Store management library based on context and observers.",
  "main": "./lib/index.js",
  "types": "./lib/types",
  "sideEffects": false,
  "private": false,
  "author": "Syrotenko Igor",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./lib && rm -rf ./target",
    "==============================": "===============================================================================",
    "build:src": "npx rollup --config ./rollup.config.js",
    "build:dts": "npx tsc -d -p ./ --outDir ./lib/types --emitDeclarationOnly",
    "build:switcher": "npx babel src/switch.ts --out-file lib/index.js --minified --compact true",
    "build": "npm run clean && npm run build:src && npm run build:switcher && npm run build:dts",
    "=============================": "================================================================================",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neloreck/dreamstate.git"
  },
  "bugs": {
    "url": "https://github.com/neloreck/dreamstate/issues"
  },
  "homepage": "https://github.com/neloreck/dreamstate#readme",
  "keywords": [
    "action",
    "class-based",
    "declarative",
    "decorator",
    "metadata",
    "react",
    "reducer",
    "redux",
    "state",
    "typescript",
    "utility"
  ],
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {
    "hoist-non-react-statics": ">=3.0.0",
    "shallow-equal": ">=1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/node": "7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-typescript": "^4.1.1",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/node": "^12.12.17",
    "@types/react": ">=16.9.16",
    "cross-env": "^7.0.2",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "rollup": "^2.6.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  }
}
