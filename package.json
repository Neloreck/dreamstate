{
  "name": "dreamstate",
  "version": "3.0.2-experimental.7",
  "description": "Store management library based on context and observers.",
  "sideEffects": false,
  "private": false,
  "author": "Syrotenko Igor",
  "license": "MIT",
  "main": "./index.js",
  "module": "./esm/index.js",
  "keywords": [
    "action",
    "class-based",
    "declarative",
    "decorator",
    "metadata",
    "react",
    "reducer",
    "redux",
    "state",
    "typescript",
    "utility"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neloreck/dreamstate.git"
  },
  "bugs": {
    "url": "https://github.com/neloreck/dreamstate/issues"
  },
  "homepage": "https://github.com/neloreck/dreamstate#readme",
  "files": [
    "index.*",
    "test-utils.*",
    "esm/",
    "types/",
    "cjs/"
  ],
  "scripts": {
    "clean": "npx rimraf ./index.* ./test-utils.* ./cjs ./esm ./coverage ./types ./portable",
    "build:debug": "npx cross-env NODE_ENV=debug rollup -c ./build/rollup.bundle.config.js",
    "build:cjs": "npx cross-env NODE_ENV=production rollup -c ./build/rollup.cjs.config.js",
    "build:esm": "npx cross-env NODE_ENV=development rollup -c ./build/rollup.esm.config.js",
    "build:dts": "npx cross-env NODE_ENV=production rollup -c ./build/rollup.dts.config.js && mv ./types/lib ./types/lib.d.ts",
    "build:portable": "npm run clean && npx cross-env NODE_ENV=production rollup -c ./build/rollup.esm.portable.config.js",
    "build": "npm run build:dts && npm run build:esm && npm run build:cjs",
    "lint": "npx eslint . --ext .ts,.js,",
    "test": "jest --config ./build/jest.config.js --detectOpenHandles",
    "prepublishOnly": "npm run clean && npm run lint && npm test && npm run build"
  },
  "dependencies": {
    "tslib": "^1.12.0",
    "hoist-non-react-statics": ">=3.0.0",
    "shallow-equal": ">=1.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@rollup/plugin-typescript": "^4.1.1",
    "@types/benchmark": "^1.0.31",
    "@types/enzyme": "^3.10.5",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.0",
    "@types/react": ">=16.8.0",
    "@types/react-dom": ">=16.8.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-jest": "^25.3.0",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^7.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.11.0",
    "jest": "^26.0.1",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^1.4.3",
    "rollup-plugin-multi-input": "^1.1.1",
    "rollup-plugin-size-snapshot": "^0.11.0",
    "rollup-plugin-terser": "^5.3.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  }
}
