{
  "name": "dreamstate",
  "version": "3.0.0-experimental.6",
  "description": "Store management library based on context and observers.",
  "sideEffects": false,
  "private": false,
  "author": "Syrotenko Igor",
  "license": "MIT",
  "files": [
    "lib/"
  ],
  "main": "./lib/cjs/dreamstate.production.js",
  "module": "./lib/esm/index.js",
  "unpkg": "./lib/umd/dreamstate.production.js",
  "types": "./lib/types/",
  "scripts": {
    "clean": "npx rimraf ./lib",
    "==============================": "z===============================================================================",
    "build:debug": "npx cross-env NODE_ENV=debug rollup -c ./build/rollup.bundle.config.js",
    "build:prod": "npx cross-env NODE_ENV=production rollup -c ./build/rollup.bundle.config.js",
    "build:esm": "npx cross-env NODE_ENV=production rollup -c ./build/rollup.esm.config.js",
    "build:dts": "npx tsc -d -p ./build --outDir ./lib/types --emitDeclarationOnly",
    "build": "npm run clean && npm run build:esm && npm run build:prod && npm run build:dts",
    "=============================": "================================================================================",
    "lint": "npx eslint src/ --ext .ts",
    "test": "jest --config ./build/jest.config.js",
    "===============================": "==============================================================================",
    "prepublishOnly": "npm run lint && npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neloreck/dreamstate.git"
  },
  "bugs": {
    "url": "https://github.com/neloreck/dreamstate/issues"
  },
  "homepage": "https://github.com/neloreck/dreamstate#readme",
  "keywords": [
    "action",
    "class-based",
    "declarative",
    "decorator",
    "metadata",
    "react",
    "reducer",
    "redux",
    "state",
    "typescript",
    "utility"
  ],
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {
    "hoist-non-react-statics": ">=3.0.0",
    "shallow-equal": ">=1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@rollup/plugin-typescript": "^4.1.1",
    "@types/enzyme": "^3.10.5",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.0",
    "@types/react": ">=16.9.16",
    "@types/react-dom": ">=16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-jest": "^25.3.0",
    "babel-plugin-macros": "^2.8.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.3.0",
    "react": "^16.13.1",
    "react-dom": ">=16.8.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.6.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-size-snapshot": "^0.11.0",
    "rollup-plugin-terser": "^5.3.0",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  }
}
